

/*Escreva um algoritmo em PORTUGOL que leia um 
número e o imprima caso ele seja maior que 20.*/

programa
{
    funcao inicio()
        {
            inteiro n
            
            escreva("Digite um número: \n")
            leia(n)
            
            se(n > 20){
                escreva(" Maior que 20 ")
                ecvreva("o número digitado foi ",n)
            }
            senao{ }
        }
}